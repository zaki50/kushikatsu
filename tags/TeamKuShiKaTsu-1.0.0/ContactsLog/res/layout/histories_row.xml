<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<TextView android:layout_height="wrap_content" android:id="@+id/txt_date"
		android:text="Date" android:layout_width="fill_parent"
		android:textSize="20sp" android:textColor="#000" />
	<TextView android:layout_height="wrap_content" android:id="@+id/txt_name"
		android:text="Name" android:layout_width="fill_parent"
		android:textSize="20sp" android:textColor="#000" />
	<TextView android:layout_height="wrap_content" android:id="@+id/txt_phone_number"
		android:text="Phone Number" android:layout_width="fill_parent"
		android:textSize="20sp" android:textColor="#000" />
</LinearLayout>
